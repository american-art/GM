f = open("Dimensions.csv","r")
fo = open("Dimensions_clean.csv","w")
ftemp = open("temp.csv","w") 
while True:
	open_flag = False
	c = f.read(1)
	print(c)
	if not c:
		break
	elif c=='"' and open_flag==False:
		open_flag = True
		ftemp.write(c)
	elif c=="\n" and open_flag==True:
		ftemp.write(" ")
	elif c=='"' and open_flag==True:
		open_flag==False
		ftemp.write(c)
		break
	else:
		ftemp.write(c)
f.close()
fo.close()
